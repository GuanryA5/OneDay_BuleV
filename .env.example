# BlueV 环境变量配置示例
# 复制此文件为 .env 并根据需要修改配置

# 调试模式
DEBUG=true

# 日志级别 (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# 数据目录
DATA_DIR=./data

# 临时文件目录
TEMP_DIR=./temp

# 应用配置
APP_NAME=BlueV
APP_VERSION=0.1.0

# 窗口配置
WINDOW_WIDTH=1200
WINDOW_HEIGHT=800
WINDOW_MIN_WIDTH=800
WINDOW_MIN_HEIGHT=600

# 性能配置
MAX_CONCURRENT_WORKFLOWS=5
IMAGE_PROCESSING_TIMEOUT=30
SCREENSHOT_QUALITY=95
